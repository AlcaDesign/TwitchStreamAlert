{
	"name": "Twitch Stream Alert",
	"description": "Alert when a channel is streaming on Twitch!",
	"version": "0.1",
	"manifest_version": 2,
	"background": {
		"scripts": [
			"scripts/jquery-2.1.4.js",
			"scripts/underscore.js",
			"background.js"
		]
	},
	"permissions": [
		"notifications",
		"storage",
		"http://static-cdn.jtvnw.net/*",
		"http://*.twitch.tv/*",
		"https://*.twitch.tv/*"
	],
	"options_ui": {
		"page": "options/index.html",
		"chrome_style": true
	}
}